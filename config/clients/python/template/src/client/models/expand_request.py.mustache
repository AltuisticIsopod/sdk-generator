class ClientExpandRequest:
    """
    ClientExpandRequest encapsulates the parameters required to expand request
    """

    def __init__(self, relation: str, object: str):
        self._relation = relation
        self._object = object

    @property
    def relation(self):
        """
        Return relation
        """
        return self._relation

    @property
    def object(self):
        """
        Return object
        """
        return self._object
